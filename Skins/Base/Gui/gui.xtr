  elseif ($6 == $chr(36)) { editbox @editbox $%($7) | setv tmp.toset $ $7 }
  elseif ($6 == id) { editbox @Editbox $ [ $+ [ $7 ] ] }
  elseif ($6 == edid) { editbox @Editbox $edid($remove($1,ð),$5) }
