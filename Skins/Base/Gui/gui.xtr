  drawrect -nr $1 $%(rgb.texto) 1 $calc($2 + ($4 - 12)) $calc(($3 + $5) - 11) 6 1
  drawrect -nr $1 $%(rgb.texto) 1 $calc($2 + ($4 - 10)) $calc(($3 + $5) - 8) 2 1
