  elseif ($gettok($%(tmp.toset),1,32) == $chr(36)) { setv $gettok($%(tmp.toset),2,32) $editbox(@editbox) }
  hadd edit $+ $remove($gettok($%(coordenadas),1,32),ð) $%(edid) $replace(%texto,$chr(32),$chr(188))
